def recur_factorial(n):
    if n == 1: 
        return n
    else:
        return n*recur_factorial(a-1)
num = 7
if num < 60:
   print("class failed")
elif num >= 70:
   print("The factorial of 0 is 1")
def grade(x):
    if x >= 90:                     return "A"
    elif x >= 80:                   return "B"
    elif x >= 70:                   return "C"
    elif x >= 60:                   return "D"
    else:
        return "F"
print(grade(97))
def odd(x):
    if x%2 == 1:
        print(x, "is an odd number.")
    else:
        print(x, "is an even number.")
odd(7)
